public class Main {
    public static void main(String[] args) {
        int numero = 5;
        cambiarValor(numero);
        System.out.println("Despu√©s de llamar a cambiarValor: " + numero);  
    }

    public static void cambiarValor(int num) {
        num = 10;
        System.out.println("Dentro de cambiarValor: " + num); 
    }
}